<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TextField?>

<VBox fx:id="root" alignment="TOP_LEFT" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.DeleteStudentController">
    <children>
        <HBox spacing="20.0">
            <children>
                <Button text="Add " onAction="#switchToPrimary" />
                <Button text="List " onAction="#switchTo_liststudent"/>
                <Button text="Find " fx:id="button3" />
                <Button text="Update " onAction="#switchTo_updatestudent" />
            </children>
        </HBox>
        <Label text="Find player dựa theo name truyền vào" />
        <TextField fx:id="text_id"></TextField>
        <Button fx:id="primaryButton" text="Find Player" onAction="#deleteStudent"/>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
